 

enum Shape {
    Circle(int),
    Rectangle(int, int),
}

fn main() {
    let s = Shape::Circle(10);
    
    match s {
        Shape::Circle(r) => {
            println("Circle with radius:");
            println(r);
        },
        Shape::Rectangle(w, h) => {
            println("Rectangle with width/height:");
            println(w);
            println(h);
        },
    }
}

