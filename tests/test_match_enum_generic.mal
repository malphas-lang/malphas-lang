 

enum Option[T] {
    Some(T),
    None
}

fn main() {
    let o = Option[int]::Some(42);
    
    match o {
        Option::Some(val) => {
            if val == 42 {
                println("Found 42");
            }
        },
        Option::None => {
            println("Found None");
        }
    };
}

