 

fn main() {
    // Basic tuple
    let t = (42, "hello");
    println(t.0);
    println(t.1);
    
    // Nested tuple
    let nested = ((1, 2), 3);
    println(nested.0.0);
    println(nested.1);
    
    // Empty tuple
    let empty = ();
    
    // Tuple with type annotation
    let typed: (int, string, bool) = (10, "world", true);
    println(typed.0);
    println(typed.1);
}
