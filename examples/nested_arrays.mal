 

fn main() {
    // Nested arrays (slices)
    let matrix: [][]int = [[1, 2], [3, 4]];
    println(matrix[0][0]);
    println(matrix[0][1]);
    println(matrix[1][0]);
    println(matrix[1][1]);
    
    // Nested string arrays
    let words: [][]string = [["hello", "world"], ["foo", "bar"]];
    println(words[0][0]);
    println(words[1][1]);
}

